<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="emp" class="com.Employee">
<property name="id" value="25">
</property>
<property name="name" value="rahul"></property>
</bean> 
 
  <bean id="stud" class="com.Student">
  
  <property name="sName" value="Praveen"></property>
  <property name="sScore" value="64"></property>
  <property name="employee" ref="emp"></property>
  
  </bean>
</beans>  